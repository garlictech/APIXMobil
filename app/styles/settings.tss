}

"Button": {
    left: 5,
    right: 5,
    height: 35
}

"#advanced_settings_button": {
    top: 400,
    title_id: 'advanced_settings'
}

"#logout_button": {
    top: 450,
    title_id: 'logout'
}

"#scroll_view": {
    contentHeight:'auto',
    showVerticalScrollIndicator: "true"
