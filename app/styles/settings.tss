    controllerName: "date_picker",
}

".query_start_date": {
    title_id: "query_interval_start",
    top: 160,
    propertyName: "QueryStartDate",
    controllerName: "date_picker"
}

".locale": {
    top: 220,
    title_id: "language",
    propertyName: "Locale",
    controllerName: "language_picker"
}

".metric_system": {
    top: 280,
    title_id: "metric_system",
    propertyName: "MetricSystem",
    controllerName: "metric_system_picker"
}

".default_query_interval": {
    title_id: "default_query_interval",
    top: 340,
    propertyName: "QueryInterval",
    controllerName: "text_input",
