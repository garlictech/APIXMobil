<Alloy>
    <Window id="window">
        <Label id="top_label" class="top bar"/>
        <!-- Ontime button added programmatically -->
        <!-- table_path added programmatically -->
        <ScrollView class="scroll_view">
            <TableView id="table" class="table"/>
        </ScrollView>
        <Label class="bottom bar" />
        <Require src="info_button"/>
